{
  "scanSettings": {
    "configMode": "AUTO",
    "configExternalURL": "",
    "projectToken": "",
    "baseBranches": [],
    "enableLicenseViolations": true
  },
  "checkRunSettings": {
    "vulnerableCheckRunConclusionLevel": "failure",
    "displayMode": "diff"
  },
  "issueSettings": {
    "minSeverityLevel": "NONE"
  },
  "remediateSettings": {
    "workflowRules": {
      "enabled": false
    },
    "enableRenovate": true,
    "extends": [
      "config:base",
      "github>whitesource/merge-confidence:beta"
      ],
      "packageRules": [
      {
        "groupName": "High Merge Confidence",
        "matchConfidence": ["high", "very high"]
      }
    ]
  }
}
